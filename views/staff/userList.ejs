<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Mental Health Counseling System</title>
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
</head>
<body>
<div class="container mt-2 text-center">
    <div class="row">
        <div class="col-12 col-md-12 offset-md-0">
            <div class="card">
                <table class="table table-hover table-striped text-center">
                    <thead class="thead-dark">
                    <tr>
                        <th>Id</th>
                        <th>Username</th>
                        <th>User type</th>
                        <th>Delete user</th>
                    </tr>
                    </thead>
                    <tbody>
                    <%for (var i =0; i<results.length;i++){%>
                    <tr>
                        <td><%=results[i].id%></td>
                        <td><%=results[i].username%></td>
                        <td><%=results[i].status%></td>
                        <td><a href="/userList/delete/<%=results[i].id%>"> Delete </a></td>
                    </tr>
                    <% }%>
                    </tbody>
                </table>
            </div>

        </div>
        <div class="col-auto">
        </div>
    </div>
</div>

<script type="application/javascript" src="/jquery/jquery.min.js"></script>
<script type="application/javascript" src="/popper/popper.min.js"></script>
<script type="application/javascript" src="/bootstrap/js/bootstrap.min.js"></script>
</body>
</html>